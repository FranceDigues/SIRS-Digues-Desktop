                  <ChoiceBox fx:id="uiOutputChoice" maxWidth="1.7976931348623157E308" minWidth="0.0" prefWidth="200.0" GridPane.rowIndex="1" />
                  <Label text="Espace disponible :" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="uiRemainingSpace" text="inconnu" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </content>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin></TitledPane>
   </bottom>
