<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<fx:root fx:id="uiBorderPane" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <center>
    <TabPane TabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
      <tabs>

<!-- ATTRIBUTE TAB-->
        <Tab text="Informations">
          <ScrollPane>
            <content>
          <VBox spacing="30.0" BorderPane.alignment="CENTER">
            <children>


<!-- FORM WHICH WILL CONTAIN ALL SIMPLE EDITORS -->


            <!-- ObligationReglementaire -->
            <GridPane hgap="10.0" styleClass="gray-light" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="NEVER" />
                <ColumnConstraints hgrow="ALWAYS" />
              </columnConstraints>
              <!--rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints-->
              <children>
                   <!-- EString -->                 
                   <Label text="%libelle" GridPane.rowIndex="0" />
                   <TextField fx:id="ui_libelle" GridPane.columnIndex="1" GridPane.rowIndex="0"  />
                   <!-- EDate -->                 
                   <Label text="%dateEcheance" GridPane.rowIndex="1" />
                   <DatePicker fx:id="ui_dateEcheance" GridPane.columnIndex="1" GridPane.rowIndex="1"  maxHeight="-Infinity" maxWidth="-Infinity"/>
                   <!-- EDate -->                 
                   <Label text="%dateRealisation" GridPane.rowIndex="2" />
                   <DatePicker fx:id="ui_dateRealisation" GridPane.columnIndex="1" GridPane.rowIndex="2"  maxHeight="-Infinity" maxWidth="-Infinity"/>
                   <!-- EInt -->                 
                   <Label text="%annee" GridPane.rowIndex="3" />
                   <Spinner fx:id="ui_annee" GridPane.columnIndex="1" GridPane.rowIndex="3"  maxHeight="-Infinity" maxWidth="-Infinity"/>
                   <Label text="%systemeEndiguementId" GridPane.rowIndex="4" />
                   <ComboBox fx:id="ui_systemeEndiguementId" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                   <Button fx:id="ui_systemeEndiguementId_link" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                   <Label text="Classe" GridPane.rowIndex="5" />
                   <TextField fx:id="ui_class" GridPane.columnIndex="1" GridPane.rowIndex="5"  />
                   <Label text="%typeId" GridPane.rowIndex="6" />
                   <ComboBox fx:id="ui_typeId" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                   <Button fx:id="ui_typeId_link" GridPane.columnIndex="2" GridPane.rowIndex="6" />
                   <Label text="%echeanceId" GridPane.rowIndex="7" />
                   <ComboBox fx:id="ui_echeanceId" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                   <Button fx:id="ui_echeanceId_link" GridPane.columnIndex="2" GridPane.rowIndex="7" />
                   <Label text="%etapeId" GridPane.rowIndex="8" />
                   <ComboBox fx:id="ui_etapeId" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                   <Button fx:id="ui_etapeId_link" GridPane.columnIndex="2" GridPane.rowIndex="8" />
                   <Label text="%planifId" GridPane.rowIndex="9" />
                   <ComboBox fx:id="ui_planifId" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                   <Button fx:id="ui_planifId_link" GridPane.columnIndex="2" GridPane.rowIndex="9" />

              </children>
              <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </padding>
            </GridPane>
            <HTMLEditor fx:id="ui_commentaire" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contentediTable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" maxHeight="-Infinity"  prefHeight="200.0" styleClass="gray-light" />

            </children>
            <padding>
              <Insets bottom="10.0" left="50.0" right="50.0" top="30.0" />
            </padding>
          </VBox>
            </content>
          </ScrollPane>
        </Tab>

<!-- REFERENCE TABS -->

      </tabs>
    </TabPane>
  </center>

</fx:root>

