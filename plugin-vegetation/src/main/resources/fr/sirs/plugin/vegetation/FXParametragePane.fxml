<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root id="BorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <SplitPane fx:id="uisplit" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <items>
            <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <center>
                  <ListView fx:id="uiPlanList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </center>
               <top>
                  <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                     <right>
                        <HBox styleClass="buttonbarfull">
                           <children>
                              <Button mnemonicParsing="false" onAction="#planAdd" styleClass="buttonbar-button" text="+" />
                              <Button mnemonicParsing="false" onAction="#planDelete" styleClass="buttonbar-button" text="-" />
                              <Button fx:id="uiDuplicate" mnemonicParsing="false" styleClass="buttonbar-button" text="2" />
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <padding>
                              <Insets left="4.0" right="4.0" top="4.0" />
                           </padding>
                        </HBox>
                     </right>
                     <center>
                        <Label maxWidth="1.7976931348623157E308" text="Plans de gestion" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                     </center>
                     <padding>
                        <Insets left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </BorderPane>
               </top>
            </BorderPane>
         </items>
      </SplitPane>
   </top>
</fx:root>
