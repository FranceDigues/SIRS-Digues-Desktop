{
        "dynamic_templates": [
            {
                "strings_not_matched": {
                    "match_pattern": "regex",
                    "match": "(?i)_.*|.*id|chemin|position.*|.*geometry.*|valid|author",
                    "match_mapping_types": "string",
                    "mapping": {
                        "type": "string",
                        "store": "no",
                        "include_in_all": false,
                        "index": "not_analyzed"
                    }
                }
            }, {
                "id_arrays": {
                    "match": "*Ids",
                    "match_mapping_types": "array",
                    "mapping": {
                        "type": "array",
                        "store": "no",
                        "include_in_all": false,
                        "index": "not_analyzed"
                    }
                }
            }
        ]
}