<fx:root fx:id="uiBorderPane" prefHeight="756.0" prefWidth="1024.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox fx:id="centerContent" BorderPane.alignment="CENTER">
            <children>
                <GridPane hgap="10.0" styleClass="gray-light" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="NEVER" />
                        <ColumnConstraints hgrow="SOMETIMES" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Nom :" />
                        <TextField fx:id="libelle" maxHeight="-Infinity" maxWidth="-Infinity" GridPane.columnIndex="1" />
                        
                        <Label minWidth="-Infinity" text="Commentaire" GridPane.valignment="TOP" GridPane.rowIndex="1" >
                            <padding>
                                <Insets top="10.0" />
                            </padding>
                        </Label>
                        <HTMLEditor fx:id="uiComment" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" prefHeight="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    
                        <Label text="%systemeEndiguementId" GridPane.rowIndex="2" />
                        <ComboBox fx:id="uiSystemeEndiguementId" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </GridPane>
                <BorderPane VBox.vgrow="NEVER">
                    <padding>
                        <Insets top="10.0" />
                    </padding>
                </BorderPane>
            </children>
        </VBox>
    </center>
</fx:root>
