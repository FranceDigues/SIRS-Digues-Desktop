<?xml version="1.0" encoding="UTF-8"?>

<?import org.geotoolkit.gui.javafx.util.*?>
<?import jidefx.scene.control.field.*?>
<?import jfxtras.scene.control.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <GridPane styleClass="pos-top" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
          <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox GridPane.columnIndex="2">
               <children>
                  <ToggleButton maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" prefWidth="120.0" selected="true" styleClass="state-button-left" text="Coordonnée" />
                  <ToggleButton maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" prefWidth="120.0" styleClass="state-button-right" text="Borne" />
               </children>
            </HBox>
            <Label styleClass="label-header" text="Positionnement" />
            <Button mnemonicParsing="false" text="I" GridPane.columnIndex="4" />
            <Button mnemonicParsing="false" text="A" GridPane.columnIndex="5" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </top>
   <center>
      <GridPane hgap="10.0" styleClass="pos-bottom" vgap="10.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" />
          <ColumnConstraints hgrow="ALWAYS" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="NEVER" />
          <RowConstraints vgrow="NEVER" />
        </rowConstraints>
         <children>
            <Label text="Systeme de Reperage" GridPane.halignment="RIGHT" />
            <ChoiceBox maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
            <GridPane hgap="5.0" vgap="5.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
              <columnConstraints>
                  <ColumnConstraints hgrow="NEVER" />
                <ColumnConstraints hgrow="ALWAYS" />
                  <ColumnConstraints hgrow="NEVER" />
                  <ColumnConstraints hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Début " GridPane.rowIndex="1" />
                  <Label text="Distance" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                  <FXNumberSpinner fx:id="uiDistanceDebut" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <FXNumberSpinner fx:id="uiDistanceFin" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                  <Label text="Fin" GridPane.rowIndex="2" />
                  <ChoiceBox maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ChoiceBox maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Amont" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
                  <Label text="Borne" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                  <CheckBox mnemonicParsing="false" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <CheckBox mnemonicParsing="false" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </center>
</fx:root>
